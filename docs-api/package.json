  },
  "devDependencies": {
    "typedoc": "^0.23.15",
    "typedoc-github-wiki-theme": "^1.0.0",
    "typedoc-gitlab-wiki-theme": "^1.0.0",
    "typedoc-plugin-markdown": "^3.11.11",
    "typedoc-plugin-markdown-pages": "^0.3.0",
    "typedoc-plugin-mdn-links": "^2.0.0",
    "typescript": "^4.7.3"
  }
}
